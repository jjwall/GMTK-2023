; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GMTK-2023"
run/main_scene="res://scenes/main_menu/main_menu.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

GameplayVars="*res://globals/gameplay_vars.gd"
DataStore="*res://globals/data_store.gd"
SceneSwitcher="*res://globals/scene_switcher.gd"
Utils="*res://globals/utils.gd"
RefData="*res://globals/ref_data.gd"
Music="*res://globals/music.tscn"
SfxDeconflicter="*res://globals/sfx_deconflicter.gd"

[display]

window/size/viewport_width=1080
window/size/viewport_height=1920
window/size/mode=2
window/size/window_width_override=720
window/size/window_height_override=1280
window/stretch/mode="viewport"
window/handheld/orientation=1

[editor_plugins]

enabled=PackedStringArray("res://addons/RPSLevelEditor/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
renderer/rendering_method.mobile="gl_compatibility"
textures/vram_compression/import_etc2_astc=true

[threading]

worker_pool/max_threads=1
